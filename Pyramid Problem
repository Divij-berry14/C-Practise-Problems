# C-Practise-Problems
Programs

#include <stdio.h>

int main()
{
   int i,j,m=5,space=4,l=0;
   for(i=1;i<=m;i++)
   {
       for(j=0;j<space;j++)
       {
           printf(" ");
       }
       for(j=1;j<=i;j++)
       {
           l=l+1;
           printf(" %d",l );
       }
       printf("\n");
       space--;
   }
   
      
    return 0;
}
