# C-Practise-Problems
Programs
#include <stdio.h>

int main()
{
   int i,j,m,l=1,k;
   printf("enter any number");
   scanf("%d",&m);
   for(i=1;i<=m;i++)
   {
       for(j=m-1;j>i;j--)
       {
           printf(" ");
       }
       for(k=1;k<=l;k++)
       {
           printf(" %d",k);
       }
       l=l+1;
       printf("\n");
      
   }
   
      
    return 0;
}


